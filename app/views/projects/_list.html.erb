<div class="center jumbotron"> 
    <% @post.each do |post| %>
    <div class = "post">
        <p class= "post-title">
            <%= post.title %>
        <p>
        
        <div class= "post-content">
            <%=raw post.content %>
                <%= link_to 'Show', project_path(post) %>
                <% if post.user_id == current_user.id%>
                    <%= link_to 'Edit', edit_project_path(post) %>
                    <%= link_to 'Delete', project_path(post), 
                                        method: :delete,
                                        data: { confirm: 'Are you sure?' } %>
                <% end %>
        </div>



        </tr>
    </div>
    <% end %>
</div>